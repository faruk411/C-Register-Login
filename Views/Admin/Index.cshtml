@model IEnumerable<Girişlogin.Identity.ApplicationUser>
@{
    ViewBag.Title = "Index";
}

<div class="panel panel-primary">
    <div class="panel-primary">
        Users
    </div>
    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Password</th>
        </tr>
        @if (Model.Count()==0)
         {
           <tr>
               <td colspan="3" class="text-center" >
                   kullanıcı yok
               </td>
           </tr>

         }
         else 
        {
            foreach(var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.UserName</td>
                        <td>@item.Email</td>
                    </tr>

                }
        
        }
        
    </table>
</div>

